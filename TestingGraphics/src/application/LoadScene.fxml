<?xml version="1.0" encoding="UTF-8"?>

<?language JavaScript?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbox" prefHeight="250.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoadController">
  <style>
	-fx-padding: 10;
  </style>
  <children>
      <Pane prefHeight="245.0" prefWidth="280.0">
         <children>
          <Label fx:id="inputLbl" alignment="CENTER_LEFT" cache="true" cacheHint="SCALE" prefHeight="30.0" prefWidth="200.0" text="Load" textAlignment="LEFT" />
            <VBox alignment="BASELINE_LEFT" layoutX="14.0" layoutY="30.0" prefHeight="200.0" prefWidth="253.0" style="-fx-spacing: 10;">
               <children>
                <Button fx:id="prevBtn" alignment="CENTER" cacheShape="false" mnemonicParsing="false" text="Main Menu" textAlignment="CENTER" />
                <Button fx:id="nextBtn" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false" text="Load Game" textAlignment="CENTER" />
               </children>
            </VBox>
         </children>
      </Pane>
  </children>
</VBox>
